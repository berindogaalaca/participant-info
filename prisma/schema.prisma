generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id                  String     @id @default(cuid())
  first_name          String     
  last_name           String
  email               String     @unique
  phone               String
  mobile              String
  gender              String
  city                String
  address             String
  country             String
  postalCode          String
  newsletter_name     String
  newsletter_email    String
  newsletter_gender   String
  isCheck             Boolean
  createdAt           DateTime   @default(now())
  updatedAt           DateTime   @updatedAt
}